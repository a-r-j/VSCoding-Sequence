{
  "name": "protein-viewer",
  "displayName": "Protein Viewer",
  "description": "",
  "version": "0.0.2",
  "publisher": "ArianJamasb",
  "author": {
    "name": "Arian Jamasb"
  },
  "icon": "img/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-r-j/vscoding-sequence.git"
  },
  "license": "MIT",
  "categories": [
    "Other",
    "Visualization",
    "Data Science"
  ],
  "activationEvents": [
    "onCommand:protein-viewer.start",
    "onCommand:protein-viewer.activateFromFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "protein-viewer.start",
        "title": "Start Protein Viewer"
      },
      {
        "command": "protein-viewer.activateFromFile",
        "title": "Launch Protein Viewer from File"
      }
    ],
    "menus": {
      "explorer/context": [{
          "when": "resourceExtname == .pdb",
          "command": "protein-viewer.activateFromFile",
          "group": "navigation"
      }]
  }
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "molstar": "^2.4.1"
  }
}
