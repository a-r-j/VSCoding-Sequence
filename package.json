{
  "name": "protein-viewer",
  "displayName": "Protein Viewer",
  "description": "Visualise 3D biological structures in the editor",
  "version": "0.1.0",
  "publisher": "ArianJamasb",
  "author": {
    "name": "Arian Jamasb"
  },
  "icon": "img/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-r-j/vscoding-sequence.git"
  },
  "license": "MIT",
  "categories": [
    "Other",
    "Visualization",
    "Data Science"
  ],
  "keywords": [
    "protein",
    "molecule",
    "biology",
    "3d",
    "structure",
    "pdb",
    "pdbqt",
    "mmcif",
    "sdf",
    "ent",
    "mol",
    "mol2",
    "xyz",
    "cif",
    "gro",
    "macromolecule",
    "bioinformatics",
    "computational biology",
    "chemistry",
    "cheminformatics",
    "computational chemistry"
  ],
  "activationEvents": [
    "onCommand:protein-viewer.start",
    "onCommand:protein-viewer.activateFromFiles",
    "onCommand:protein-viewer.activateFromFolder",
    "onCommand:protein-viewer.ESMFold",
    "onCommand:commandId:protein-viewer.activateFromActiveEditor"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "protein-viewer.start",
        "title": "Start Protein Viewer"
      },
      {
        "command": "protein-viewer.activateFromFiles",
        "title": "Launch Protein Viewer from File(s)"
      },
      {
        "command": "protein-viewer.activateFromFolder",
        "title": "Launch Protein Viewer from Folder"
      },
      {
        "command": "protein-viewer.activateFromActiveEditor",
        "title": "Protein Viewer: View Current File"
      },
      {
        "command": "protein-viewer.ESMFold",
        "title": "ESMFold Sequence"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .pdb || resourceExtname == .pdb.gz || resourceExtname == .PDB || resourceExtname == .mol2 || resourceExtname == .MOL2 || resourceExtname == .sdf || resourceExtname == .SDF || resourceExtname == .mmCIF || resourceExtname == .mmcif || resourceExtname == .cif.gz || resourceExtname == .MMCIF || resourceExtname == .Mol || resourceExtname == .mol || resourceExtname == .MOL || resourceExtname == .xyz || resourceExtname == .XYZ || resourceExtname == .ent || resourceExtname == .ENT || resourceExtname == .pdbqt || resourceExtname == .PDBQT || resourceExtname == .cif || resourceExtname == .CIF || resourceExtname == .mcif || resourceExtname == .MCIF || resourceExtname == .gro || resourceExtname == .GRO",
          "command": "protein-viewer.activateFromFiles",
          "group": "navigation"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "protein-viewer.activateFromFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "glob": "^9.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "molstar": "^4.5.0",
    "node-fetch": "^2.6.1",
    "@types/vscode": "^1.91.0"
  }
}
